{
  "name": "elda-test",
  "version": "1.0.0",
  "description": "this is a technical test for elda technology",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "npm run dev --prefix backend",
    "dev:client": "npm run dev --prefix frontend",
    "build": "npm run build --prefix backend && npm run build --prefix frontend",
    "start": "npm run server:start --prefix backend",
    "env:dev": "node -e \"require('fs').copyFileSync('backend/.env.dev','backend/.env')\" && node -e \"require('fs').copyFileSync('frontend/.env.dev','frontend/.env')\"",
    "init": "npm install && npm install --prefix backend && npm install --prefix frontend && npm run init --prefix backend && npm run env:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsebdev/elda_test.git"
  },
  "keywords": [
    "elda"
  ],
  "engines": {
    "node": ">24"
  },
  "author": "Sebastien GAULT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gsebdev/elda_test/issues"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
